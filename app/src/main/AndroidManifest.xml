        </activity>
        <activity android:name=".Activity.SecondActivity" />

        <service
            android:name=".Cards.Card_Services"
            android:exported="false" />

        <activity android:name=".Activity.ChamanActivity" />
        <activity android:name=".Activity.ChasseurActivity" />
        <activity android:name=".Activity.MageActivity" />
        <activity android:name=".Activity.PretreActivity" />
        <activity android:name=".Activity.DemonisteActivity" />
        <activity android:name=".Activity.DruideActivity" />
        <activity android:name=".Activity.GuerrierActivity" />
        <activity android:name=".Activity.PaladinActivity" />
        <activity android:name=".Activity.VoleurActivity" />
        <activity android:name=".Activity.SelectionChamanActivity" />
        <activity android:name=".Activity.LegendaireActivity"></activity>
    </application>

</manifest>